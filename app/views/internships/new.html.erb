<%= simple_form_for [@company, @internship] do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :length %>
    <%= f.input :hours %>
  </div>


  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
